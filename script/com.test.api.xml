<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<configuration version="1">
  <!--java -cp "test.jar" unit_test.Main - -testscript="script_name.xml"-->
  <suite enable="true" loopcount="1" name="UniqueName">
    <com>
      <test name="com.test.api.testcase.TC_LaunchApi"/>
      <test name="com.test.api.testcase.TC_BrandExist"/>
      <test name="com.test.api.testcase.TC_BrandNotExist"/>
      <test name="com.test.api.testcase.TC_NumOfBrand"/>
      <test name="com.test.api.testcase.TC_NumOfSubCatgory"/>
    </com>
    <featurefiles>
      <file glue="" name="testscenarios.feature"/>
    </featurefiles>
    <parameters>
      <parameter name="PARAMETER_0">parameterValue_0</parameter>
      <parameter name="PARAMETER_1">parameterValue_1</parameter>
      <parameter name="PARAMETER_2">parameterValue_2</parameter>
    </parameters>
    <testcasegroups>
      <group name="*"/>
    </testcasegroups>
    <testunitgroups>
      <group name="*"/>
    </testunitgroups>
  </suite>
</configuration>
